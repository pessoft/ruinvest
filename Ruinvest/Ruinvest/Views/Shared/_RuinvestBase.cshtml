    <meta charset="utf-8" />
    @Styles.Render("~/Content/css")
    @Scripts.Render("~/scripts/jquery-3.0.0.js")
    @Scripts.Render("~/scripts/jquery.unobtrusive-ajax.js")
    @Scripts.Render("~/bundles/jqueryval")
    @Scripts.Render("~/bundles/bootstrap")
    <link rel="shortcut icon" href="~/Image/title-ico.png" type="image/png">
    <link href="~/Content/main.css" rel="stylesheet" />
    <link href="~/Content/fm.revealator.jquery.css" rel="stylesheet" />
    <script src="~/Scripts/main.js"></script>
    <script src="~/Scripts/fm.revealator.jquery.js"></script>
